{
  "root": "packages/mime-aware",
  "sourceRoot": "packages/mime-aware/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/mime-aware",
        "main": "packages/mime-aware/src/index.ts",
        "tsConfig": "packages/mime-aware/tsconfig.lib.json",
        "assets": ["packages/mime-aware/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/mime-aware/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/mime-aware"],
      "options": {
        "jestConfig": "packages/mime-aware/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
