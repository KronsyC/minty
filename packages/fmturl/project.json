{
    "root": "packages/fmturl",
    "sourceRoot": "packages/fmturl/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/packages/fmturl",
                "main": "packages/fmturl/src/index.ts",
                "tsConfig": "packages/fmturl/tsconfig.lib.json",
                "assets": ["packages/fmturl/*.md"]
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["packages/fmturl/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/packages/fmturl"],
            "options": {
                "jestConfig": "packages/fmturl/jest.config.js",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
